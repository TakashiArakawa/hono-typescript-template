{
  "name": "hono-typescript-template",
  "version": "1.0.0",
  "description": "Modern Hono API server with TypeScript, Zod validation, and Vitest testing",
  "scripts": {
    "check:types": "tsc --noEmit",
    "check:lint": "eslint .",
    "check:format": "prettier . --check",
    "fix:format": "prettier . --write",
    "test": "vitest --run",
    "start": "tsx src/index.ts",
    "check:all": "npm run check:types && npm run check:lint && npm run fix:format && npm test"
  },
  "keywords": [
    "hono",
    "typescript",
    "api",
    "rest",
    "zod",
    "vitest",
    "template",
    "starter"
  ],
  "author": "Takashi Arakawa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TakashiArakawa/hono-typescript-template.git"
  },
  "homepage": "https://github.com/TakashiArakawa/hono-typescript-template#readme",
  "bugs": {
    "url": "https://github.com/TakashiArakawa/hono-typescript-template/issues"
  },
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.4",
    "@hono/zod-validator": "^0.7.0",
    "hono": "^4.8.3",
    "zod": "^3.25.67"
  }
}
